{
  "name": "vlib-kit",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prepare": "echo 'Preparing package...' && husky install",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "commit": "npx git-cz",
    "docs:dev": "pnpm --filter=docs docs:dev",
    "docs:clean": "pnpm --filter=docs docs:clean",
    "docs:build": "pnpm --filter=docs docs:build",
    "docs:serve": "pnpm --filter=docs docs:serve",
    "gen": "pnpm --filter=@vlib/ui gen",
    "build": "pnpm --filter=@vlib/ui build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@vlib/eslint-config": "workspace:*",
    "@vlib/stylelint-config": "workspace:*",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.40.0",
    "eslint-define-config": "^1.20.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.1",
    "typescript": "^5.0.4",
    "unplugin-vue-define-options": "^1.3.5"
  }
}
